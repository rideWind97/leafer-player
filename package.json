{
  "name": "leafer-canvas-player",
  "version": "0.1.0",
  "description": "A Leafer-based canvas video player component.",
  "license": "MIT",
  "author": "",
  "keywords": [
    "leafer-player"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rideWind97/leafer-player.git"
  },
  "homepage": "https://github.com/rideWind97/leafer-player#readme",
  "bugs": {
    "url": "https://github.com/rideWind97/leafer-player/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "@leafer-in/flow": ">=1",
    "leafer-editor": ">=1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@rollup/plugin-url": "^8.0.2",
    "rollup": "^4.55.1",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
